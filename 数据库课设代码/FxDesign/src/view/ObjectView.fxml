<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="ObjectView" prefHeight="657.0" prefWidth="1070.0" style="-fx-background-image: url(img/ObjectView.jpg); -fx-background-size: 100%,100%;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ObjectView">
   <children>
      <ImageView fitHeight="66.0" fitWidth="85.0" layoutX="6.0" layoutY="14.0" style="-fx-image: url(img/first.png);">
         <image>
            <Image url="@../img/first.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="39.0" fitWidth="200.0" layoutX="77.0" layoutY="28.0" style="-fx-image: url(img/second.png);">
         <image>
            <Image url="@../img/second.png" />
         </image>
      </ImageView>
      <TextArea fx:id="content" layoutX="144.0" layoutY="129.0" prefHeight="449.0" prefWidth="781.0" wrapText="true">
         <effect>
            <InnerShadow />
         </effect>
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font></TextArea>
      <TextField fx:id="Title" layoutX="385.0" layoutY="72.0" prefHeight="55.0" prefWidth="300.0">
         <effect>
            <InnerShadow />
         </effect>
         <font>
            <Font name="System Bold Italic" size="30.0" />
         </font></TextField>
      <Button fx:id="okButton" layoutX="642.0" layoutY="588.0" mnemonicParsing="false" onAction="#okButton" prefHeight="55.0" prefWidth="148.0" text="确定修改">
         <font>
            <Font name="System Bold Italic" size="20.0" />
         </font>
      </Button>
      <Button fx:id="lastButton" layoutX="277.0" layoutY="588.0" mnemonicParsing="false" onAction="#lastButton" prefHeight="55.0" prefWidth="148.0" text="返回上一级">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
